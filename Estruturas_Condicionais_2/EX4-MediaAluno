#include <stdio.h>

int main(void) {
  float media, n1, n2;

  printf("Informe a primeira nota: ");
  scanf("%f", &n1);

  printf("Informe a segunda nota: ");
  scanf("%f", &n2);

  media = (n1 + n2)/2;

  printf("A media do aluno Ã©: %f\n", media);

  if((media <= 10) && (media >= 9)){
    printf("Aluno nota A");
  }
  else{
    if((media <= 8) && (media >= 7)){
      printf("Aluno nota B"); 
    }
    else{
      if((media <= 6) && (media >= 5)){
        printf("Aluno nota C");
      }
      else{
        if((media <= 4) && (media >= 3)){
          printf("Aluno nota D");
        }
        else
          printf("Aluno nota F");
      }
    }
  }
  return 0;
}