#include <stdio.h>

int main(void) {
  float massa, altura, imc;

  printf("Informe sua altura: ");
  scanf("%f", &altura);

  printf("Informe seu peso: ");
  scanf("%f", &massa);

  imc = massa / (altura * altura);

  printf("Seu imc é de: %2.f\n", imc);
  
  if((imc>=18.5) && (imc<=25)){
    printf("Voce esta no peso ideal");
  }
    else{
      printf("Voce não esta no peso ideal");
    }
  
  return 0;
}